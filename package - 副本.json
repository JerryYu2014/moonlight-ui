{
  "name": "moonlight-ui-x",
  "version": "1.0.0",
  "description": "Moonlight UI is a component library based on Vue",
  "main": "./src/index.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && mocha --reporter spec",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha test/*.js - - -R spec",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config config/webpack.dev.js",
    "build2": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.build.js",
    "package": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.npm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JerryYu2014/moonlight-ui.git"
  },
  "keywords": [
    "ui",
    "vue"
  ],
  "author": "advoster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JerryYu2014/moonlight-ui/issues"
  },
  "homepage": "https://github.com/JerryYu2014/moonlight-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.0.1",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.2",
    "typescript": "^3.9.7",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "html-webpack-plugin": "^4.3.0",
    "moonlight-utils": "^1.0.10",
    "postcss-loader": "^3.0.0",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
